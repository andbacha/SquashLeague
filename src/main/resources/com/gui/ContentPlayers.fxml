<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.ContentPlayers">
   <children>
      <HBox spacing="5.0">
         <children>
            <Label text="Zawodnicy">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Region HBox.hgrow="ALWAYS" />
          <Button fx:id="buttonAddPlayer" mnemonicParsing="false" onAction="#addPlayer" text="Dodaj" />
            <Button fx:id="buttonModifyPlayer" mnemonicParsing="false" onAction="#modifyPlayer" text="Modyfikuj" />
            <Button fx:id="buttonRemovePlayer" mnemonicParsing="false" onAction="#removePlayer" text="UsuÅ„" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <ListView fx:id="listViewPlayers" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
